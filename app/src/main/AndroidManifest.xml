<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LibraryActivity"
            android:label="@string/library" />
        <activity
            android:name=".RecentActivity"
            android:label="@string/recent" />
        <activity
            android:name=".PodcastActivity"
            android:label="@string/podcast" />
        <activity
            android:name=".DiscoverActivity"
            android:label="@string/discover" />
        <activity
            android:name=".NowPlayingActivity"
            android:label="@string/NowPlaying" />
        <activity android:name=".PlayBarActivity" />
        <activity
            android:name=".PaymentActivity"
            android:label="@string/payment"/>
    </application>

</manifest>